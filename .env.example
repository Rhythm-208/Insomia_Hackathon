# .env — copy this file and fill in your values
# NEVER commit this file to git

SECRET_KEY=django-insecure-changethisbeforedeploy
DEBUG=True

# Gemini — from aistudio.google.com
GEMINI_API_KEY=your_gemini_api_key_here

# Google OAuth — from Google Cloud Console credentials.json
GOOGLE_CLIENT_ID=your_client_id_here.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your_client_secret_here

# MongoDB — local for development
MONGO_URI=mongodb://localhost:27017/

# OAuth redirect — must match Google Cloud Console setting
REDIRECT_URI=http://localhost:8000/auth/callback/
